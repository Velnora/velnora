{
  "name": "@velnora/router",
  "version": "0.0.0.dev-0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/velnora.router.d.ts",
      "import": "./build/velnora.router.js"
    },
    "./client": {
      "types": "./build/client/velnora.router.client.d.ts",
      "import": "./build/client/velnora.router.client.js"
    },
    "./server": {
      "types": "./build/server/velnora.router.server.d.ts",
      "import": "./build/server/velnora.router.server.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@velnora/types": "workspace:^"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "debug": "^4.4.3",
    "eslint": "^9.39.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=24"
  },
  "publishConfig": {
    "access": "public"
  }
}
