{
  "name": "@velnora/router",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/velnora.router.d.ts",
      "import": "./build/velnora.router.js"
    },
    "./client": {
      "types": "./build/client/velnora.router.client.d.ts",
      "import": "./build/client/velnora.router.client.js"
    },
    "./server": {
      "types": "./build/server/velnora.router.server.d.ts",
      "import": "./build/server/velnora.router.server.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "jsdoc": "jsdoc2md -f ./src/**/*.ts",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@velnora/schemas": "workspace:^"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "eslint": "^9.39.1",
    "tsup": "^8.5.1",
    "type-fest": "^5.3.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=24"
  }
}
