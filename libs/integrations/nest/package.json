{
  "name": "@velnora/integrations-nest",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/velnora.nest.d.ts",
      "import": "./build/velnora.nest.js"
    },
    "./server": {
      "types": "./build/velnora.server.d.ts",
      "import": "./build/velnora.server.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "lint": "eslint .",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@nestjs/platform-express": "^11.1.9",
    "@velnora/devkit": "workspace:^",
    "@velnora/plugin-api": "workspace:^",
    "@velnora/types": "workspace:^"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "@nestjs/common": "^11",
    "@nestjs/core": "^11",
    "rxjs": "^7"
  },
  "engines": {
    "node": ">=24"
  }
}
